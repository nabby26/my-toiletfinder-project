<p id="notice"><%= notice %></p>

<h1>Toilets</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Description</th>
      <th>Parentsroom</th>
      <th>Gender neutral</th>
      <th>Disabled opt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @toilets.each do |toilet| %>
      <tr>
        <td><%= toilet.title %></td>
        <td><%= toilet.location %></td>
        <td><%= toilet.description %></td>
        <td><%= toilet.parentsRoom %></td>
        <td><%= toilet.gender_neutral %></td>
        <td><%= toilet.disabled_opt %></td>
        <td><%= link_to 'Show', toilet %></td>
        <td><%= link_to 'Edit', edit_toilet_path(toilet) %></td>
        <td><%= link_to 'Destroy', toilet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Toilet', new_toilet_path %>
